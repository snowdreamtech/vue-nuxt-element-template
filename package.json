{
  "name": "vue-nuxt-element-template",
  "packageManager": "pnpm@8.7.5",
  "version": "0.1.0",
  "description": "Vue3+Nuxt3+Element Plus(template)",
  "repository": {
    "type": "git",
    "url": "git@github.com:snowdreamtech/vue-nuxt-element-template.git"
  },
  "author": "SnowdreamTech",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "MODE=dev nuxi dev",
    "stage": "MODE=staging nuxi dev",
    "build": "MODE=prod nuxi build",
    "preview": "MODE=prod nuxi preview",
    "start": "MODE=prod node .output/server/index.mjs",
    "generate": "MODE=prod nuxi generate",
    "postinstall": "MODE=prod nuxi prepare",
    "lint": "eslint --fix --ext .js,.ts,.vue .",
    "msw": "npx msw init public/ --save"
  },
  "dependencies": {
    "@element-plus/icons-vue": "2.1.0",
    "element-plus": "2.3.12",
    "js-cookie": "3.0.5",
    "normalize.css": "8.0.1",
    "ofetch": "1.3.3",
    "path-browserify": "1.0.1",
    "path-to-regexp": "6.2.1"
  },
  "devDependencies": {
    "@element-plus/nuxt": "1.0.6",
    "@faker-js/faker": "8.0.2",
    "@nuxt/devtools": "0.8.3",
    "@nuxtjs/color-mode": "3.3.0",
    "@nuxtjs/eslint-config-typescript": "12.1.0",
    "@nuxtjs/eslint-module": "4.1.0",
    "@nuxtjs/robots": "3.0.0",
    "@pinia/nuxt": "0.4.11",
    "@unocss/nuxt": "0.55.7",
    "@vueuse/nuxt": "10.4.1",
    "consola": "3.2.3",
    "dotenv": "16.3.1",
    "eslint": "8.49.0",
    "msw": "1.3.0",
    "nuxt": "3.7.2",
    "nuxt-icon": "0.5.0",
    "rollup-plugin-node-polyfills": "0.2.1",
    "sass": "1.66.1",
    "typescript": "5.2.2"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "webpack",
        "vite",
        "vue"
      ]
    }
  },
  "msw": {
    "workerDirectory": "public"
  }
}