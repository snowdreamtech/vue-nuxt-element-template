{
  "name": "element-plus-nuxt-starter",
  "packageManager": "pnpm@8.6.2",
  "version": "0.1.0",
  "description": "Element Plus with Nuxt 3 Minimal Starter",
  "repository": {
    "type": "git",
    "url": "git://github.com/element-plus/element-plus-nuxt-starter"
  },
  "author": "Element Plus Team",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "MODE=dev nuxi dev",
    "stage": "MODE=staging nuxi dev",
    "build": "MODE=prod nuxi build",
    "preview": "MODE=prod nuxi preview",
    "start": "MODE=prod node .output/server/index.mjs",
    "generate": "MODE=prod nuxi generate",
    "postinstall": "MODE=prod nuxi prepare",
    "msw": "npx msw init public/ --save"
  },
  "dependencies": {
    "@element-plus/icons-vue": "2.1.0",
    "element-plus": "2.3.9",
    "js-cookie": "3.0.5",
    "normalize.css": "8.0.1",
    "ofetch": "1.3.2",
    "path-browserify": "1.0.1",
    "path-to-regexp": "6.2.1",
    "screenfull": "^6.0.2"
  },
  "devDependencies": {
    "@element-plus/nuxt": "1.0.5",
    "@faker-js/faker": "8.0.2",
    "@nuxtjs/color-mode": "3.3.0",
    "@nuxtjs/i18n": "8.0.0-rc.3",
    "@pinia/nuxt": "0.4.11",
    "@unocss/nuxt": "0.55.2",
    "@vueuse/nuxt": "10.3.0",
    "consola": "3.2.3",
    "dotenv": "16.3.1",
    "msw": "1.2.3",
    "nuxt": "3.6.5",
    "nuxt-icon": "0.5.0",
    "sass": "1.66.1",
    "typescript": "5.1.6"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "webpack",
        "vite",
        "vue"
      ]
    }
  },
  "msw": {
    "workerDirectory": "public"
  }
}